<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/sml/ns/j2ee"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
		web-jsptaglibrary_2_0.xsd">
<tlib-version>1.0</tlib-version>		
<short-name>fake</short-name>
<uri>http://ekunt.com/jsp/jstl/fake</uri>
<tag>
	<name>if</name>
	<tag-class>com.ekunt.tag.IfTag</tag-class>
	<body-content>scriptless</body-content>
	<attribute>
		<name>test</name>
		<type>boolean</type>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
</tag>

<tag>
	<name>forEach</name>
	<tag-class>com.ekunt.tag.ForEachTag</tag-class>
	<body-content>scriptless</body-content>
	<attribute>
		<name>var</name>
		<type>String</type>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
	<attribute>
		<name>items</name>
		<type>java.util.Collection</type>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
</tag>

<tag>
	<name>toUpperCase</name>
	<tag-class>com.ekunt.tag.ToUpperCaseTag</tag-class>
	<body-content>scriptless</body-content>
</tag>


<tag>
	<name>choose</name>
	<tag-class>com.ekunt.tag.ChooseTag</tag-class>
	<body-content>scriptless</body-content>
</tag>

<tag>
	<name>when</name>
	<tag-class>com.ekunt.tag.WhenTag</tag-class>
	<body-content>scriptless</body-content>
	<attribute>
		<name>test</name>
		<type>boolean</type>
		<required>true</required>
		<rtexprvalue>true</rtexprvalue>
	</attribute>
</tag>

<tag>
	<name>otherwise</name>
	<tag-class>com.ekunt.tag.OtherwiseTag</tag-class>
	<body-content>scriptless</body-content>
</tag>

</taglib>